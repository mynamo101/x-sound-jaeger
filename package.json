{
  "name": "elysian",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run sync-js && astro dev",
    "dev-watch": "concurrently \"npm run watch-js\" \"astro dev\"",
    "sync-js": "sync-js.bat",
    "watch-js": "powershell -ExecutionPolicy Bypass -File watch-js.ps1",
    "build": "npm run sync-js && astro build && upload.bat",
    "build-only": "npm run sync-js && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prebuild": "powershell -Command \"Set-Content .env \"SITE_VERSION=$(Get-Date -Format yyyyMMddHHmmss)\""
  },
  "dependencies": {
    "@astrojs/mdx": "^4.2.1",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.3.0",
    "astro": "^5.5.3",
    "astro-auto-import": "^0.4.4",
    "astro-seo": "^0.8.4",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mdast-util-to-string": "^4.0.0",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "reading-time": "^1.5.0",
    "wavesurfer.js": "^7.9.5"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}
